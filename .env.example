# browser-automation-service 环境配置

# ==================================
# 应用配置
# ==================================
APP__ENV=dev
APP__DEBUG=true
APP__PORT=1111

# ==================================
# 数据库配置
# ==================================
DATABASE__HOST=localhost
DATABASE__PORT=5432
DATABASE__USER=postgres
DATABASE__PASSWORD=your_password
DATABASE__DATABASE=browser_automation
DATABASE__SCHEMA_NAME=public
DATABASE__MAX_CONNECTIONS=10
DATABASE__MIN_CONNECTIONS=1

# ==================================
# 日志配置
# ==================================
LOGGER__LEVEL=INFO
LOGGER__TO_FILE=false
LOGGER__TO_CONSOLE=true
LOGGER__FILE_PATH=logs/app.log
LOGGER__FILE_ROTATION='1 day'
LOGGER__FILE_RETENTION='30 days'


# ==================================
# AgentBay 配置
# ==================================
AGENTBAY__API_KEY=your_agentbay_api_key_here
AGENTBAY__BASE_URL=https://your-agentbay-instance.com

# ==================================
# 外部服务配置
# ==================================
EXTERNAL_SERVICE__EZLINK_BASE_URL=https://api.ezlinkai.com/v1
EXTERNAL_SERVICE__EZLINK_API_KEY=your_ezlink_api_key
EXTERNAL_SERVICE__VECTAI_BASE_URL=https://api.vectortara.com/v1
EXTERNAL_SERVICE__VECTAI_API_KEY=your_vectorai_api_key

# ==================================
# 安全配置
# ==================================
SECURITY__ENCRYPTION_KEY=your_32_byte_hex_key_here

# ==================================
# OSS配置
# ==================================
OSS__ACCESS_KEY_ID=your_oss_access_key_id
OSS__ACCESS_KEY_SECRET=your_oss_access_key_secret
OSS__ENDPOINT=https://oss-cn-beijing.aliyuncs.com
OSS__BUCKET_NAME=your_bucket_name